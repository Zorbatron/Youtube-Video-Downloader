<Window x:Class="YoutubeVideoDownloader.QualitySelector"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YoutubeVideoDownloader"
        mc:Ignorable="d"
        Title="QualitySelector" Height="400" Width="400" WindowStartupLocation="CenterScreen" Loaded="QualSelectorLoaded">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="10"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="10"/>
        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition Height="10"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="10"/>
        </Grid.RowDefinitions>

        <TextBlock Visibility="Hidden" Grid.Column="0" Grid.Row="0" Name="IReallyHopeThisCodeWorks"/>

        <ScrollViewer Grid.Column="1" Grid.Row="1">
            <StackPanel Name="StackPanel"/>
        </ScrollViewer>
    </Grid>
</Window>
